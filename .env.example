# PostgreSQL Database Configuration
REACT_APP_DB_HOST=localhost
REACT_APP_DB_PORT=5432
REACT_APP_DB_NAME=agentic_sales_db
REACT_APP_DB_USER=your_db_user
REACT_APP_DB_PASSWORD=your_db_password

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id
REACT_APP_FIREBASE_MEASUREMENT_ID=your_measurement_id

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:3001/api
REACT_APP_EMBEDDING_API_URL=http://localhost:3002

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_REAL_TIME_COLLAB=true
REACT_APP_ENABLE_VECTOR_SEARCH=true